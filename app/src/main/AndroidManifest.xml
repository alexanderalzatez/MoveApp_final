<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.susanatoro.moveapp">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_move_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_move_launcher"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".NavDrawerActivity"
                android:label="@string/title_activity_nav_drawer"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Registro4Activity">
        </activity>
        <activity android:name=".Registro3Activity">
        </activity>
        <activity android:name=".Registro2Activity">
        </activity>
        <activity android:name=".UbicacionActivity">
        </activity>
        <activity
                android:name=".RegistroActivity"
                android:label="@string/app_registro">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity
                android:name=".SplashActivity"
                android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>